/* Hamburger menu icon styling */
.navbar-toggler {
    border: 2px solid rgba(0, 0, 0, 0.5);
    padding: 5px 10px;
    background-color: transparent;
}

.navbar-toggler:focus {
    outline: none;
    box-shadow: none;
}

.navbar-toggler-icon {
    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' width='30' height='30' viewBox='0 0 30 30'%3e%3cpath stroke='rgba%280, 0, 0, 0.7%29' stroke-linecap='round' stroke-miterlimit='10' stroke-width='2' d='M4 7h22M4 15h22M4 23h22'/%3e%3c/svg%3e");
    display: inline-block;
    width: 25px;
    height: 25px;
}
/* Show hamburger menu only for screens < 900px */
@media only screen and (max-width: 899px) {
    .navbar-toggler {
        display: block;
    }
    
    /* Ensure proper mobile navigation styling */
    .navbar-nav {
        text-align: center;
        padding: 15px 0;
    }
    
    .navbar-nav .nav-item {
        margin: 10px 0;
    }
}

/* Keep existing media queries */
@media only screen and (min-width: 992px) {
    .navbar .navbar-nav .nav-item .nav-link {
        padding: 0 0.5em;
    }

    .navbar .navbar-nav .nav-item:not(:last-child) .nav-link {
        border-right: 0.5px solid #bebcbc;
    }
    .navbar-toggler {
        color:rgba(0, 0, 0, 0.7)
    }
    
}
@media only screen and (max-width: 1200px) {
    .container {
        padding: 5px;
        margin: auto;
    }
    .info-section {
        padding-top: 100px;
        padding-bottom: 80px;
    }
    .card {
        width: 800px;
    }
    .col-md-6 {
        -ms-flex: 0 0 100%;
        flex: 0 0 100%;
        max-width: 100%;
    }
    img {
        margin: 0 auto;
    }
    .container-wrap {
        min-height: 100%;
        padding: 10px;
    }
    #officeCarousel {
        max-width: 100%;
        margin: 0 auto;

    }
}
@media only screen and (max-width: 700px) {
    
    body, .card {
        width: 100%;
    }
    
}






/* Prevent horizontal overflow on all screen sizes */
.container, .container-fluid {
    max-width: 100%;
    overflow-x: hidden;
}

.row {
    margin-left: 0;
    margin-right: 0;
    max-width: 100%;
}

.col-md-6, .col-md-12, .col-md-4 {
    padding-left: 15px;
    padding-right: 15px;
}

/* Ensure images don't overflow */
img {
    max-width: 100%;
    height: auto;
}

.image-section img {
    width: 100%;
    object-fit: cover;
}

/* Text wrapping and mobile fixes */
* {
    word-wrap: break-word;
    overflow-wrap: break-word;
}

/* Fix accordion on mobile */
@media only screen and (max-width: 900px) {
    .accordion-button {
        width: 100%;
        max-width: 100%;
        white-space: normal;
        text-align: left;
        padding: 10px 15px;
        word-wrap: break-word;
    }
    
    .accordion-content {
        width: 100%;
        max-width: 100%;
        padding: 15px;
        word-wrap: break-word;
    }
    
    .card {
        width: 100%;
        max-width: 100%;
        margin: 10px 0;
    }
    
    .centered-accordion {
        padding: 80px 10px 100px 10px;
        width: 100%;
    }
    
    /* Fix carousel controls on mobile */
    .carousel-control-prev-icon:after,
    .carousel-control-next-icon:after {
        font-size: 30px;
    }
    
    .carousel-control-prev-icon,
    .carousel-control-next-icon {
        height: 50px;
        width: 50px;
    }
    
    /* Fix container-wrap for carousel */
    .container-wrap {
        padding: 0;
        min-height: auto;
        width: 100%;
    }
    
    #officeCarousel {
        width: 100%;
        max-width: 100%;
    }
    
    .carousel-item img {
        width: 100%;
        height: auto;
        min-height: 300px;
        object-fit: cover;
    }
}

/* Fix footer text wrapping on mobile */
@media only screen and (max-width: 767px) {
    .fixed-footer {
        height: auto;
        min-height: 80px;
        padding: 10px 0;
    }
    
    .fixed-footer .container-fluid {
        flex-direction: column;
        text-align: center;
        padding: 10px;
    }
    
    .fixed-footer .container-fluid > div {
        margin: 5px 0;
        font-size: 12px;
        word-wrap: break-word;
        max-width: 100%;
    }
}

/* Ensure all text content wraps properly */
p, li, a, span, div, h1, h2, h3, h4, h5, h6 {
    word-wrap: break-word;
    overflow-wrap: break-word;
    hyphens: auto;
}

/* Fix strategy cards on mobile */
@media only screen and (max-width: 767px) {
    .strategy-img {
        width: 100% !important;
        max-width: 100%;
        margin: 10px 0;
    }
}

/* Additional card responsiveness for about page */
@media only screen and (max-width: 767px) {
    .card {
        width: 100% !important;
        max-width: 100% !important;
        height: auto !important;
        margin: 10px auto;
    }
    
    .card-body {
        padding: 15px;
        word-wrap: break-word;
    }
}

/* Fix info-text on all screen sizes */
.info-text {
    word-wrap: break-word;
    overflow-wrap: break-word;
}

/* Ensure proper spacing on mobile for all pages */
@media only screen and (max-width: 767px) {
    body {
        font-size: 16px;
    }
    
    .info-text {
        font-size: 16px;
        padding: 15px;
    }
    
    h1 {
        font-size: 28px;
    }
    
    h2 {
        font-size: 24px;
    }
    
    h3 {
        font-size: 20px;
    }
}

/* Fix body padding for fixed footer */
body {
    padding-bottom: 120px; /* Account for fixed footer */
}

/* Override Bootstrap's navbar-expand-md to work at our 900px breakpoint */
@media (max-width: 899px) {
    .navbar-expand-md > .container,
    .navbar-expand-md > .container-fluid {
        padding-right: 0;
        padding-left: 0;
    }
    
    .navbar-expand-md .navbar-nav {
        flex-direction: column;
    }
    
    .navbar-expand-md .navbar-nav .nav-link {
        padding-right: 0;
        padding-left: 0;
    }
    
    .navbar-expand-md > .container,
    .navbar-expand-md > .container-fluid {
        flex-wrap: wrap;
    }
    
    .navbar-expand-md .navbar-collapse {
        display: block !important;
        flex-basis: 100%;
        flex-grow: 1;
        align-items: center;
    }
    
    .navbar-expand-md .navbar-toggler {
        display: block;
    }
}

@media (min-width: 900px) {
    .navbar-expand-md {
        flex-flow: row nowrap;
        justify-content: flex-start;
    }
    
    .navbar-expand-md .navbar-nav {
        flex-direction: row;
    }
    
    .navbar-expand-md .navbar-collapse {
        display: flex !important;
        flex-basis: auto;
    }
    
    .navbar-expand-md .navbar-toggler {
        display: none;
    }
}

/* Ensure collapsed menu is hidden by default */
.navbar-collapse.collapse:not(.show) {
    display: none;
}

/* When the menu is shown (has .show class), display it */
.navbar-collapse.collapse.show {
    display: block;
}

/* Make sure the collapsed menu appears below the navbar on mobile */
@media (max-width: 899px) {
    .navbar-collapse {
        width: 100%;
        background-color: rgb(240, 241, 241);
        padding: 10px 0;
    }
}

/* Increase bottom padding on mobile for taller footer */
@media only screen and (max-width: 767px) {
    body {
        padding-bottom: 180px;
    }
}

@media only screen and (min-width: 768px) and (max-width: 991px) {
    body {
        padding-bottom: 140px;
    }
}

/* Lock hero overlay text size - prevent accessibility text scaling from affecting it */
.overlay-text {
    /* Prevent text size adjustments from accessibility settings */
    -webkit-text-size-adjust: none;
    -moz-text-size-adjust: none;
    -ms-text-size-adjust: none;
    text-size-adjust: none;
    
    /* Force fixed font sizes */
    font-size: 38px !important;
    
    /* Prevent wrapping */
    white-space: nowrap;
    
    /* Override any zoom/scale settings */
    zoom: 1;
}

.sharada-text {
    /* Prevent text size adjustments from accessibility settings */
    -webkit-text-size-adjust: none;
    -moz-text-size-adjust: none;
    -ms-text-size-adjust: none;
    text-size-adjust: none;
    
    /* Force fixed font size */
    font-size: 100px !important;
    
    /* Prevent wrapping */
    white-space: nowrap;
}

/* Override for mobile - still keep it fixed but smaller */
@media only screen and (max-width: 767px) {
    .overlay-text {
        font-size: 24px !important;
        white-space: normal;
        max-width: 90vw;
    }
    
    .sharada-text {
        font-size: 50px !important;
        white-space: normal;
    }
}

/* Medium screens */
@media only screen and (min-width: 768px) and (max-width: 991px) {
    .overlay-text {
        font-size: 30px !important;
        white-space: nowrap;
    }
    
    .sharada-text {
        font-size: 70px !important;
        white-space: nowrap;
    }
}

/* Improved hamburger menu behavior */
.navbar-collapse {
    transition: height 0.35s ease, opacity 0.35s ease;
}

/* Ensure proper collapse behavior */
.navbar-collapse.collapsing {
    height: 0;
    overflow: hidden;
    transition: height 0.35s ease;
}

.navbar-collapse.show {
    height: auto !important;
}

/* Smooth animation for mobile menu */
@media (max-width: 899px) {
    .navbar-collapse {
        max-height: 0;
        overflow: hidden;
        transition: max-height 0.35s ease-in-out;
    }
    
    .navbar-collapse.show {
        max-height: 500px;
        transition: max-height 0.35s ease-in-out;
    }
    
    .navbar-collapse.collapsing {
        max-height: 0;
    }
}

/* Prevent body scroll when menu is open on mobile */
@media (max-width: 899px) {
    body.menu-open {
        overflow: hidden;
    }
}

/* Universal fix for fixed header overlap - simpler approach */
/* Add top margin to all direct children after fixed-header */
.fixed-header ~ * {
    margin-top: 100px;
}

